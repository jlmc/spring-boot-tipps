type Query {
    helloworld: String
    greet(name: String!): String!
    gerRandomNumbers: [Int!]!
    getBook: BookResource!
    getPosts: [PostResource!]!
}

type BookResource {
    id: ID,
    title: String
}

type PostResource {
    id: ID
    title: String!
    description: String
    author: UserResource
}

type UserResource {
    id: ID!
    name: String!
    posts: [PostResource!]!
}
